generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model projects {
  id             Int     @id @default(autoincrement())
  title          String
  image          String?
  description    String?
  version        String?
  other_versions String?
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  password String

  @@map("users")
}
